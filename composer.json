{
    "name": "secode-framework/apifirst-generator",
    "description": "The Secode Framework.",
    "keywords": ["framework", "laravel","secode"],
    "license": "MIT",
    "homepage": "https://www.secode.com",
    "support": {
        "issues": "https://github.com/Secode-Framework/api-generator/issues",
        "source": "https://github.com/Secode-Framework/api-generator"
    },
    "authors": [
        {
            "name": "Hans De La Cruz Acosta",
            "email": "hansdelacruzacosta@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/yaml": "6.2.*",
        "nette/php-generator": "3.6.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.5.8"
    },
    "provide": {
        "psr/container-implementation": "1.1|2.0",
        "psr/simple-cache-implementation": "1.0|2.0|3.0"
    },
    "autoload": {
        "psr-4": {
            "Secode\\": "src/Secode/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Secode\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": [
            "vendor/bin/phpunit --coverage-html test-coverage"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
